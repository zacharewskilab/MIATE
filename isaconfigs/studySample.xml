<isatab-config-file xmlns="http://www.ebi.ac.uk/bii/isatab_configuration#">
	<isatab-configuration table-name="studySample">
		<measurement term-label="[Sample]" term-accession="" source-abbreviation=""/>
		<technology term-label="" term-accession="" source-abbreviation=""/>
		<field header="Source Name" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Sources are considered as the starting biological material used in a study.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Characteristics[organism]" data-type="Ontology term" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="true">
			<description>
				<![CDATA[Provide a taxonomic information associated to the source biological material. This should be included as an NCBI TaxID unless not found within NCBI]]>
			</description>
			<default-value/>
		</field>
		<field header="Characteristics[strain]" data-type="Ontology term" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="true">
			<description>
				<![CDATA[Provide strain information associated to the source biological material. This should be included using an ontology to ensure consistency in nomenclature]]>
			</description>
			<default-value/>
		</field>
		<field header="Characteristics[sex]" data-type="List" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Sex of the study organism. Must be one of the following: FEMALE, MALE]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
			<list-values>FEMALE,MALE</list-values>
		</field>
		<protocol-field protocol-type ="Animal housing"/>
		<field header="Parameter Value[start age]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Age of the organism at the time the first dose was administered.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[end age]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Age of the organism when the terminal sample was collected.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<unit-field data-type="String" is-multiple-value="false" is-required="true" is-forced-ontology="true">
			<default-value/>
			<description>organism age unit"</description>
		</unit-field>
		<field header="Parameter Value[temperature]" data-type="Double" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Average vivarium temperature during the study]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<unit-field data-type="String" is-multiple-value="false" is-required="false" is-forced-ontology="true">
			<default-value/>
			<description>Temperature unit"</description>
		</unit-field>
		<field header="Parameter Value[humidity]" data-type="Double" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Average vivarium humidity during the study]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<unit-field data-type="String" is-multiple-value="false" is-required="false" is-forced-ontology="true">
			<default-value/>
			<description>Huminiduty unit"</description>
		</unit-field>
		<field header="Parameter Value[light cycle]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Vivarium light cycle for the study period defined as light:dark formattet as number:number (e.g. 12:12, 03:21, ...)]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[cage]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Define the type of caging used for the study period]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[bedding]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Define the type of bedding used for the study period]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[water]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Define the water source for the study period]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="Diet exposure"/>
		<field header="Parameter Value[Feed description]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Description of feed including, but not limited to, macronutrient composition]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[Feed source]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Vendor of the feed tested if commercially available]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[Feed catalog number]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Catalog number of the feed if commercially available.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Factor Value[Feed name]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Descriptive name of the diet]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="Substance treatment"/>
		<field header="Parameter Value[substance]" data-type="Ontology term" is-file-field="false" is-multiple-value="true" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[The name of the substance tested.]]>
			</description>
			<default-value/>
			<recommended-ontologies>
				<ontology id="http://data.bioontology.org/ontologies/CHEBI" abbreviation="CHEBI" name="Chemical Entities of Biological Interest Ontology" version="80"/>
			</recommended-ontologies>
		</field>
		<field header="Parameter Value[vehicle substance]" data-type="Ontology term" is-file-field="false" is-multiple-value="true" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[The name of the vehicle compound used to deliver the toxicant.]]>
			</description>
			<default-value/>
		</field>
		<field header="Parameter Value[administration interval]" data-type="Double" is-file-field="false" is-multiple-value="true" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Amount of time between subsequent doses when multiple doses are administered over a period of time.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<unit-field data-type="String" is-multiple-value="false" is-required="false" is-forced-ontology="false">
			<default-value/>
			<description>Administration interval unit"</description>
		</unit-field>
		<field header="Parameter Value[administration number]" data-type="Integer" is-file-field="false" is-multiple-value="true" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[The number of doses administered when multiple doses are given over a period of time.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[administration route]" data-type="List" is-file-field="false" is-multiple-value="true" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[The delivery mechanism for the toxicant tested.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
			<list-values>GAVAGE,INTRAPERITONEAL,RETRO-ORBITAL,TAIL VEIN INJECTION,DIET,WATER,DOUGH PILL</list-values>
		</field>
		<field header="Parameter Value[chemical cat no]" data-type="String" is-file-field="false" is-multiple-value="true" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Catalog number of the toxicant.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[chemical vendor]" data-type="String" is-file-field="false" is-multiple-value="true" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Vendor of the toxicant tested]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[purity]" data-type="String" is-file-field="true" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Chemical purity of the toxicant. A sample information sheet from the vendor can be attached.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Factor Value[dose]" data-type="Double" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Dose of the toxicant]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<unit-field data-type="String" is-multiple-value="false" is-required="false" is-forced-ontology="true">
			<default-value></default-value>
			<description>Dose unit"</description>
		</unit-field>
		<protocol-field protocol-type ="Euthanasia"/>
		<field header="Parameter Value[Fasting duration]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Amount of time animals were fasted prior to euthanisation.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[Euthanasia method]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Method used for euthanasia.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Factor Value[time point]" data-type="Double" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Time point at which the sample was collected.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<unit-field data-type="String" is-multiple-value="false" is-required="true" is-forced-ontology="false">
			<default-value></default-value>
			<description>Time-point unit"</description>
		</unit-field>
		<field header="Sample Name" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Samples represent major outputs resulting from a protocol application other than the special case outputs of Extract or a Labeled Extract.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<structured-field name="characteristics"/>
		<structured-field name="factors"/>
	</isatab-configuration>
</isatab-config-file>