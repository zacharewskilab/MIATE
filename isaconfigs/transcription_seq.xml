				<![CDATA[
				
				
				
			
			
			]]>
			</default-value>
			<list-values>RAW COUNTS,TPM,CPM,FOLD-CHANGE,RPKM,FPKM</list-values>
		</field>
		<structured-field name="factors"/>
	</isatab-configuration>
</isatab-config-file>