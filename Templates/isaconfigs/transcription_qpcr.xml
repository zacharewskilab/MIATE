<isatab-config-file xmlns="http://www.ebi.ac.uk/bii/isatab_configuration#">
	<isatab-configuration table-name="transcription_qpcr" isatab-assay-type="generic_assay" isatab-conversion-target="generic">
		<measurement term-label="transcription profiling" term-accession="http://purl.obolibrary.org/obo/OBI_0000424" source-abbreviation="OBI"/>
		<technology term-label="real time PCR" term-accession="http://purl.obolibrary.org/obo/OBI_0000893" source-abbreviation="OBI"/>
		<field header="Sample Name" data-type="String" is-file-field="false" is-multiple-value="true" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Samples represent major outputs resulting from a protocol application other than the special case outputs of Extract or a Labeled Extract.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Characteristics[Tissue]" data-type="Ontology term" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="true">
			<description>
				<![CDATA[The tissue from which nucleic acids were isolated.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="Protocol for nucleic acid extraction"/>
		<field header="Parameter Value[Preservation]" data-type="Ontology term" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Method of sample preservation (e.g. frozen, FFPE)]]>
			</description>
			<default-value/>
			<recommended-ontologies>
				<ontology id="http://data.bioontology.org/ontologies/OBI" abbreviation="OBI" name="Ontology for Biomedical Investigations" version="22"/>
			</recommended-ontologies>
		</field>
		<field header="Characteristics[Nuclei acid purity]" data-type="Double" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Nucleic acid purity (A260/A280)]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Characteristics[RIN]" data-type="Double" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[RNA integrity number]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Parameter Value[RIN method]" data-type="List" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Method or equipment used to calculate RNA integrity number]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
			<list-values>Gel electrophoresis,Agilent 2100 Bioanalyzer,Agilent 2200 TapeStation,Agilent 4200 TapeStation</list-values>
		</field>
		<field header="Characteristics[RIN Traces]" data-type="String" is-file-field="true" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[trace]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Extract Name" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[User-defined names for each portion of extracted material.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="Protocol for reverse transcription"/>
		<field header="cDNA Extract Name" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Identifier for the labeled extract.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="Target probe design"/>
		<field header="Parameter Value[Target name]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Name of the target sequence. Use official gene symbol from the appropriate authority (e.g. HGNC, MGI)]]>
			</description>
			<default-value/>
		</field>
		<field header="Parameter Value[Target accession id]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[RefSeq, Entrex, or Ensembl accession id of the gene of interest.]]>
			</description>
			<default-value/>
		</field>
		<field header="Parameter Value[Genome build]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Provide the ]]>
			</description>
			<default-value/>
		</field>
		<field header="Parameter Value[forward primer sequence]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[forward primer sequence]]>
			</description>
			<default-value/>
		</field>
		<field header="Parameter Value[reverse primer sequence]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[reverse primer sequence]]>
			</description>
			<default-value/>
		</field>
		<field header="Parameter Value[probe sequence]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[probe sequence]]>
			</description>
			<default-value/>
		</field>
		<field header="Assay Name" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[User-defined name for an assay.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
			<generated-value-template>[INSTITUTION].Group-[GROUP_NO].Subject-[SUBJECT_NO].[SAMPLE_EXTRACT].Extract-[EXTRACT_COUNT].LE-[LABEL_COUNT].ASSAY-[HYB_COUNT]</generated-value-template>
		</field>
		<protocol-field protocol-type ="qPCR protocol"/>
		<field header="Parameter Value[Label]" data-type="Ontology term" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Indicates a chemical or biological marker, such as a radioactive isotope or a fluorescent dye which is bound to a material in order to make it detectable by some assay technology (e.g. P33, biotin, GFP).]]>
			</description>
			<default-value/>
		</field>	
		<field header="Parameter Value[Instrument]" data-type="String" is-file-field="false" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[qPCR instrument model]]>
			</description>
			<default-value/>
		</field>	
		<field header="qPCR protocol file" data-type="String" is-file-field="true" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Upload the protocol file if one is created.]]>
			</description>
			<default-value/>
		</field>		
		<protocol-field protocol-type ="data collection"/>
		<field header="Characteristics[Well]" data-type="String" is-file-field="false" is-multiple-value="true" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[well location in output file]]>
			</description>
			<default-value/>
		</field>
		<field header="Raw Data File" data-type="String" is-file-field="true" is-multiple-value="false" is-required="true" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Name (or URI) of the raw data file generated by an assay.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="data transformation"/>
		<field header="Normalization Name" data-type="String" is-file-field="false" is-multiple-value="false" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[User-defined name for each normalization applied]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<protocol-field protocol-type ="data transformation"/>
		<field header="Data Transformation Name" data-type="String" is-file-field="false" is-multiple-value="true" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[User-defined name for each data transformation applied]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<field header="Derived Data File" data-type="String" is-file-field="true" is-multiple-value="true" is-required="false" is-hidden="false" is-forced-ontology="false">
			<description>
				<![CDATA[Name (or URI) of the data file generated by an assay.]]>
			</description>
			<default-value>
				<![CDATA[]]>
			</default-value>
		</field>
		<structured-field name="factors"/>
	</isatab-configuration>
</isatab-config-file>